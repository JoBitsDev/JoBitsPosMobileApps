<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:icon="@drawable/pos_dpte"
        android:label="@string/app_name"
        >

        <activity
            android:name="com.activities.OrdenReadOnlyActivity"
            android:label="@string/title_activity_orden"
            android:launchMode="singleTop"></activity>
        <activity
            android:name="com.activities.OrdenActivity"
            android:label="@string/title_activity_orden"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustNothing"></activity>
        <activity
            android:name="com.activities.PantallaPrincipalActivity"
            android:label="@string/title_activity_activity_principal"
            android:parentActivityName="com.activities.MainActivity"
            android:configChanges="screenSize|orientation">//TODO: Que no llame de nuevo al OnCreate cuando se vire la pantalla
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.activities.MainActivity" />
        </activity>
    </application>

</manifest>