<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="firstdream.restaurantmanageralmacen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoapp"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <service
            android:name="com.main.service.ReceiverService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".Main"
            android:label="@string/app_name">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:launchMode="singleTask" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".about"
            android:label="@string/title_activity_about"
            android:parentActivityName=".Main"
            android:theme="@android:style/Theme.Dialog">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="firstdream.restaurantmanageralmacen.Main" />
        </activity>
        <activity
            android:name=".almacenstate"
            android:label="Almacen"
            android:parentActivityName=".Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="firstdream.restaurantmanageralmacen.Login" />
        </activity>
    </application>

</manifest>